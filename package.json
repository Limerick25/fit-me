{
  "name": "master-shredder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "save": "node scripts/auto-commit.js",
    "backup": "node scripts/auto-commit.js 'Manual backup checkpoint'",
    "commit": "node scripts/auto-commit.js",
    "sync": "git pull origin main && node scripts/auto-commit.js 'Sync with remote changes'",
    "watch": "node scripts/auto-watcher.js",
    "auto": "concurrently \"npm run dev\" \"npm run watch\"",
    "start": "npm run auto",
    "deploy": "node scripts/deploy.js",
    "deploy-auto": "node scripts/vercel-auto-deploy.js",
    "deploy-prod": "npm run build && vercel --prod",
    "setup-vercel": "vercel",
    "update-model": "node scripts/update-claude-model.js",
    "update-model-opus": "node scripts/update-claude-model.js opus",
    "update-model-sonnet": "node scripts/update-claude-model.js sonnet",
    "test-models": "node scripts/test-models.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
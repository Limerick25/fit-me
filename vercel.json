{
  "name": "master-shredder",
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "VITE_CLAUDE_API_KEY": "@vite_claude_api_key",
    "VITE_CLAUDE_MODEL": "@vite_claude_model"
  },
  "build": {
    "env": {
      "VITE_CLAUDE_API_KEY": "@vite_claude_api_key",
      "VITE_CLAUDE_MODEL": "@vite_claude_model"
    }
  },
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}